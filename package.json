{
  "name": "mbo-theme-tool",
  "private": true,
  "version": "0.0.7",
  "type": "commonjs",
  "main": "electron/main.js",
  "author": "Chiko",
  "description": "一个基于 Vue3 + Electron 的简单应用，用于公司内部项目快速新增主题。",
  "scripts": {
    "dev": "vite",
    "h": "vite build && electron .",
    "build": "vue-tsc && vite build",
    "build:prod": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev\" \"npm run electron:serve\"",
    "electron:serve": "cross-env NODE_ENV=development electron .",
    "electron:build": "npm run build:prod && electron-builder",
    "electron:build:win": "npm run build:prod && electron-builder --win",
    "electron:build:mac": "npm run build:prod && electron-builder --mac",
    "electron:build:linux": "npm run build:prod && electron-builder --linux",
    "clean": "rimraf dist dist_electron node_modules/.vite",
    "postinstall": "electron-builder install-app-deps",
    "build-icons": "electron-icon-builder -i ./build/logo.png -o ./build/"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^4.2.6",
    "electron-updater": "^6.6.2",
    "pinia": "^2.0.34",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "electron-icon-builder": "^2.0.1",
    "rimraf": "^5.0.0",
    "sass": "^1.89.2",
    "terser": "^5.43.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  },
  "packageManager": "pnpm@10.12.2+sha512.a32540185b964ee30bb4e979e405adc6af59226b438ee4cc19f9e8773667a66d302f5bfee60a39d3cac69e35e4b96e708a71dd002b7e9359c4112a1722ac323f"
}
